{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "display": "Fastify",
  "compilerOptions": {
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "strictBindCallApply": false,
    "target": "esnext",
    "types": ["node"]
  }
}
